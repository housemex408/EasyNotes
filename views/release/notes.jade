//http://naltatis.github.io/jade-syntax-docs/
extends ../layout
block content
    div(class='navbar navbar-inverse navbar-fixed-top', role='navigation')
        div.container
            div.navbar-header
    div.jumbotron
        div.container
            div.starter-template
                h2= version
            h3 Stories
            ul
                for story in stories
                    li= story.fields.summary
            h3 Chores
            ul
                for chore in chores
                    li= chore.fields.summary
            h3 Bugs
            ul
                for bug in bugs
                    li= bug.fields.summary
